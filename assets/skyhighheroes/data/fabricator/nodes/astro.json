{
  "suit" : "skyhighheroes:astro",
  "requirements": [
    "skyhighheroes:robot",
    "fiskheroes:tier8_9"
  ],
  "criteria": {
    "death": {
      "trigger": "fiskheroes:player_killed",
      "description": {
        "translate": "Die by collision with another player while both players are boost flying"
      },
      "condition": {
        "variables": {
          "player": "PLAYER_KILLED",
          "other_player": "DAMAGE_SOURCE"
          },
        "check": "player.isWearingFullSuit() && other_player.getEntity().as('PLAYER').isWearingFullSuit() && player.getData('fiskheroes:flight_boost_timer') == 1 && other_player.getEntity().as('PLAYER').getData('fiskheroes:flight_boost_timer') == 1 && other_player.hasDamageType('COLLISION')"
      }
    }
  }
}