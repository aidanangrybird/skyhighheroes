      "values": ["base", "long", "short", "normal"]
    },
    "OPENINGS": {
      "key": "(entity.getData('skyhighheroes:dyn/body_upper_front_open_timer') > 0 || entity.getData('skyhighheroes:dyn/body_upper_back_open_timer') > 0) ? 'upper' : (entity.getData('skyhighheroes:dyn/body_lower_front_open_timer') > 0 || entity.getData('skyhighheroes:dyn/body_lower_back_open_timer') > 0) ? 'lower' : (entity.getData('skyhighheroes:dyn/body_machine_gun_open_timer') > 0) ? 'lowest' : 'none'",
